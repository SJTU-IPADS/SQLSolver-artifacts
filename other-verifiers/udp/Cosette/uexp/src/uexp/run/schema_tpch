schema region(r_regionkey:int, r_name:int, r_comment:int);
schema nation(n_nationkey:int, n_name:int, n_regionkey:int, n_comment:int);
schema part(p_partkey:int, p_name:int, p_mfgr:int, p_brand:int, p_type:int, p_size:int, p_container:int, p_retailprice:int, p_comment:int);
schema supplier(s_suppkey:int, s_name:int, s_address:int, s_nationkey:int, s_phone:int, s_acctbal:int, s_comment:int);
schema partsupp(ps_partkey:int, ps_suppkey:int, ps_availqty:int, ps_supplycost:int, ps_comment:int);
schema customer(c_custkey:int, c_name:int, c_address:int, c_nationkey:int, c_phone:int, c_acctbal:int, c_mktsegment:int, c_comment:int);
schema orders(o_orderkey:int, o_custkey:int, o_orderstatus:int, o_totalprice:int, o_orderdate:int, o_orderpriority:int, o_clerk:int, o_shippriority:int, o_comment:int);
schema lineitem(l_orderkey:int, l_partkey:int, l_suppkey:int, l_linenumber:int, l_quantity:int, l_extendedprice:int, l_discount:int, l_tax:int, l_returnflag:int, l_linestatus:int, l_shipdate:int, l_commitdate:int, l_receiptdate:int, l_shipinstruct:int, l_shipmode:int, l_comment:int);

table warehouse(warehouse);
table item(item);
table stock(stock);
table district(district);
table customer(customer);
table history(history);
table oorder(oorder);
table new_order(new_order);
table order_line(order_line);

unique(region, r_regionkey);
unique(nation, n_nationkey);
unique(part, p_partkey);
unique(supplier, s_suppkey);
unique(customer, c_custkey);
unique(orders, o_orderkey);
foreign(nation, n_regionkey, region, r_regionkey);
foreign(supplier, s_nationkey, nation, n_nationkey);
foreign(partsupp, ps_partkey, part, p_partkey);
foreign(partsupp, ps_suppkey, supplier, s_suppkey);
foreign(customer, c_nationkey, nation, n_nationkey);
foreign(orders, o_custkey, customer, c_custkey);
foreign(lineitem, l_orderkey, orders, o_orderkey);
